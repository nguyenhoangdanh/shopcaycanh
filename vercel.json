{
  "version": 2,
  "name": "shop-cay-canh",
  "builds": [
     { "src": "./server.js", "use": "@vercel/node" },
     { "src": "*.html", "use": "@vercel/static" },
     { "src": "*.py", "use": "@vercel/python" },
     { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
     { "src": "/(.*)", "dest": "./server.js" }
  ]
}